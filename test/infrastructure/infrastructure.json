{
    "repo_url": "git@github.com:grapeup/gu_cf_infrastructure_azure_deployment.git",
    "repo_version": "s3-proxy-docker",
    "repo_private_key_var_name": "git_private_key",

    "pipelines": [
        {
            "name": "bosh_dev",
            "file": "ci/pipelines/dev_pipeline.yml",
            "vars": [
                {"name": "trigger_on_repo_change", "value": "false"}
            ]
        },
        {
            "name": "bosh_backup",
            "file": "ci/pipelines/backup_pipeline.yml",
            "vars": [
                {"name": "backup_interval", "value": "48h"}
            ]
        },
        {
            "name": "bosh_restore",
            "file": "ci/pipelines/restore_pipeline.yml"
        },
        {
            "name": "env_destroy",
            "file": "ci/pipelines/destroy_pipeline.yml"
        }
    ],

    "vars": [
        {"name": "bosh_public_key", "fortress_var": "bosh_public_key"},

        {"name": "network_cidr", "value": "10.91.0.0/16"},
        {"name": "bosh_subnet_cidr", "value": "10.91.1.0/24"},
        {"name": "dmz_subnet_cidr", "value": "10.91.2.0/24"},
        {"name": "ops_subnet_cidr", "value": "10.91.0.0/24"},
        {"name": "tools_subnet_cidr", "value": "10.91.10.0/24"},
        {"name": "cf_subnet_cidr", "value": "10.91.16.0/20"},

        {"name": "jumpbox_public_key", "fortress_var": "jumpbox_public_key"},
        {"name": "jumpbox_git_private_key", "fortress_var": "git_private_key"},

        {"name": "git_private_key", "fortress_var": "git_private_key"},

        {"name": "bosh_gateway_ip", "value": "10.91.1.1"},
        {"name": "bosh_private_key", "fortress_var": "bosh_private_key"},
        {"name": "bosh_reserved_ips", "value": "10.91.1.2-10.91.1.3"},

        {"name": "cf_gateway_ip", "value": "10.91.16.1"},
        {"name": "cf_reserved_ips", "value": "10.91.16.2-10.91.16.3"},

        {"name": "dmz_gateway_ip", "value": "10.91.2.1"},
        {"name": "dmz_reserved_ips", "value": "10.91.2.2-10.91.2.3"},

        {"name": "tools_gateway_ip", "value": "10.91.10.1"},
        {"name": "tools_reserved_ips", "value": "10.91.10.2-10.91.10.3"},

        {"name": "fortress_network_id", "fortress_var": "network_id"},
        {"name": "fortress_network_cidr", "fortress_var": "network_cidr"},
        {"name": "fortress_network_name", "fortress_var": "network_name"},
        {"name": "fortress_resource_group_name", "fortress_var": "resource_group_name"},

        {"name": "concourse_cert_password", "value": ""},
        {"name": "jumpbox_allowed_cidr_1", "value": "79.190.200.98/32"},
        {"name": "jumpbox_allowed_cidr_2", "value": "193.106.245.10/32"}
    ]
}
