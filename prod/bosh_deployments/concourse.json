{
  "repo_url": "git@github.com:grapeup/gu_concourse_deployment.git",
  "repo_version": "master",
  "repo_private_key_var_name": "git_private_key",

  "pipelines": [
    {
      "name": "deploy_concourse",
      "file": "ci/pipelines/azure/deploy_pipeline.yml",
      "vars": [
        {"name": "trigger_on_repo_change", "value": "false"},
        {"name": "concourse_bbr_release", "value": "concourse-bbr/concourse-bbr-1.0.1.tgz"}
      ]
    },
    {
      "name": "backup_concourse",
      "file": "ci/pipelines/azure/backup_pipeline.yml",
      "vars": [
        {"name": "backup_interval", "value": "48h"}
      ]
    },
    {
      "name": "restore_concourse",
      "file": "ci/pipelines/azure/restore_pipeline.yml",
      "vars": []
    },
    {
      "name": "concourse_dev",
      "file": "ci/pipelines/azure/dev_pipeline.yml",
      "vars": [
        {"name": "trigger_on_repo_change", "value": "false"},
        {"name": "concourse_bbr_release", "value": "concourse-bbr/concourse-bbr-1.0.1.tgz"}
      ]
    }
  ],

  "vars": [
    {"name": "backups_container_name", "value": "test-container"},
    {"name": "backups_storage_account_name", "value": "grapeupmatrteststorage"},
    {"name": "backups_storage_account_key", "value": "3B4s5cdL9FV3UPIFTZKR6ULFBMQ4S3Kok+EV5piaQx+70cFQOo1EArnXPID8lqOBR6PL3pPRIcY/Tw4ljnI6DQ=="}
  ]
}
