{
    "repo_url": "git@github.com:grapeup/gu_cf_infrastructure_azure_deployment.git",
    "repo_version": "master",
    "repo_private_key_var_name": "git_private_key",

    "pipelines": [
        {
            "name": "deploy_env",
            "file": "ci/pipelines/deploy_pipeline.yml",
            "vars": [
                {"name": "trigger_on_repo_change", "value": "false"}
            ]
        },
        {
            "name": "destroy_env",
            "file": "ci/pipelines/destroy_pipeline.yml",
            "vars": []
        }
    ],

    "vars": [
        {"name": "bosh_public_key", "fortress_var": "bosh_public_key"},

        {"name": "network_cidr", "value": "10.80.0.0/16"},
        {"name": "bosh_subnet_cidr", "value": "10.80.1.0/24"},
        {"name": "dmz_subnet_cidr", "value": "10.80.2.0/24"},
        {"name": "ops_subnet_cidr", "value": "10.80.0.0/24"},
        {"name": "tools_subnet_cidr", "value": "10.80.10.0/24"},
        {"name": "cf_subnet_cidr", "value": "10.80.16.0/20"},

        {"name": "jumpbox_ami", "fortress_var": "jumpbox_ami"},
        {"name": "jumpbox_public_key", "fortress_var": "jumpbox_public_key"},
        {"name": "jumpbox_git_private_key", "fortress_var": "git_private_key"},

        {"name": "git_private_key", "fortress_var": "git_private_key"},
        {"name": "bosh_backup_bucket_name", "value": "backups.cflab.grapeup.com"},
        {"name": "bosh_backup_bucket_region", "fortress_var": "s3_region"},
        {"name": "bosh_backup_s3_access_key", "fortress_var": "s3_access_key"},
        {"name": "bosh_backup_s3_secret_key", "fortress_var": "s3_secret_key"},

        {"name": "bosh_gateway_ip", "value": "10.80.1.1"},
        {"name": "bosh_private_key", "fortress_var": "bosh_private_key"},
        {"name": "bosh_reserved_ips", "value": "10.80.1.2-10.80.1.3"},

        {"name": "cf_gateway_ip", "value": "10.80.16.1"},
        {"name": "cf_reserved_ips", "value": "10.80.16.2-10.80.16.3"},

        {"name": "dmz_gateway_ip", "value": "10.80.2.1"},
        {"name": "dmz_reserved_ips", "value": "10.80.2.2-10.80.2.3"},

        {"name": "tools_gateway_ip", "value": "10.80.10.1"},
        {"name": "tools_reserved_ips", "value": "10.80.10.2-10.80.10.3"},

        {"name": "fortress_network_id", "fortress_var": "network_id"},
        {"name": "fortress_network_name", "fortress_var": "network_name"},
        {"name": "fortress_resource_group_name", "fortress_var": "resource_group_name"}
    ]
}
