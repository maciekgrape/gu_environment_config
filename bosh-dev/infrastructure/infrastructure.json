{
    "repo_url": "git@github.com:grapeup/gu_cf_infrastructure_aws_deployment.git",
    "repo_version": "master",
    "repo_private_key_var_name": "git_private_key",

    "pipelines": [
      {
          "name": "bosh_dev",
          "file": "ci/pipelines/dev_pipeline.yml",
          "vars": [
              {"name": "trigger_on_repo_change", "value": "false"}
          ]
      }
    ],

    "vars": [
        {"name": "bosh_public_key", "fortress_var": "bosh_public_key"},
        {"name": "bosh_subnet_cidr", "value": "10.92.1.0/24"},
        {"name": "cf_01_subnet_cidr", "value": "10.92.16.0/20"},
        {"name": "cf_02_subnet_cidr", "value": "10.92.32.0/20"},

        {"name": "cf_certificate_arn", "fortress_var": "cf_certificate_arn"},
        {"name": "concourse_certificate_arn", "fortress_var": "concourse_certificate_arn"},

        {"name": "dmz_01_subnet_cidr", "value": "10.92.2.0/24"},
        {"name": "dmz_02_subnet_cidr", "value": "10.92.3.0/24"},

        {"name": "fortress_aws_access_key", "fortress_var": "aws_access_key"},
        {"name": "fortress_aws_account_id", "fortress_var": "aws_account_id"},
        {"name": "fortress_aws_region", "fortress_var": "aws_region"},
        {"name": "fortress_aws_secret_key", "fortress_var": "aws_secret_key"},
        {"name": "fortress_cidr", "fortress_var": "vpc_cidr"},
        {"name": "fortress_vpc_id", "fortress_var": "vpc_id"},
        {"name": "fortress_tools_01_route_table_id", "fortress_var": "tools_01_route_table_id"},
        {"name": "fortress_tools_02_route_table_id", "fortress_var": "tools_02_route_table_id"},

        {"name": "jumpbox_ami", "fortress_var": "jumpbox_ami"},
        {"name": "jumpbox_public_key", "fortress_var": "jumpbox_public_key"},
        {"name": "jumpbox_git_private_key", "fortress_var": "git_private_key"},

        {"name": "network_cidr", "value": "10.92.0.0/16"},

        {"name": "ops_subnet_cidr", "value": "10.92.0.0/24"},

        {"name": "tools_01_subnet_cidr", "value": "10.92.10.0/24"},
        {"name": "tools_02_subnet_cidr", "value": "10.92.11.0/24"},

        {"name": "git_private_key", "fortress_var": "git_private_key"},
        {"name": "bosh_backup_bucket_name", "value": "backups.cflab.grapeup.com"},
        {"name": "bosh_backup_bucket_region", "value": "eu-central-1"},
        {"name": "bosh_backup_s3_access_key", "fortress_var": "aws_access_key"},
        {"name": "bosh_backup_s3_secret_key", "fortress_var": "aws_secret_key"},

        {"name": "bosh_gateway_ip", "value": "10.92.1.1"},
        {"name": "bosh_private_key", "fortress_var": "bosh_private_key"},
        {"name": "bosh_reserved_ips", "value": "10.92.1.2-10.92.1.3"},

        {"name": "cf_01_gateway_ip", "value": "10.92.16.1"},
        {"name": "cf_01_reserved_ips", "value": "10.92.16.2-10.92.16.3"},

        {"name": "cf_02_gateway_ip", "value": "10.92.32.1"},
        {"name": "cf_02_reserved_ips", "value": "10.92.32.2-10.92.32.3"},

        {"name": "dmz_01_gateway_ip", "value": "10.92.2.1"},
        {"name": "dmz_01_reserved_ips", "value": "10.92.2.2-10.92.2.3"},

        {"name": "dmz_02_gateway_ip", "value": "10.92.3.1"},
        {"name": "dmz_02_reserved_ips", "value": "10.92.3.2-10.92.3.3"},

        {"name": "tools_01_gateway_ip", "value": "10.92.10.1"},
        {"name": "tools_01_reserved_ips", "value": "10.92.10.2-10.92.10.3"},

        {"name": "tools_02_gateway_ip", "value": "10.92.11.1"},
        {"name": "tools_02_reserved_ips", "value": "10.92.11.2-10.92.11.3"}
    ]
}
