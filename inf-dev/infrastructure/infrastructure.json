{
    "repo_url": "git@github.com:grapeup/gu_infrastructure_deployment.git",
    "repo_version": "master",
    "repo_private_key_var_name": "git_private_key",

    "pipelines": [
        {
            "name": "infrastructure_dev",
            "file": "ci/pipelines/dev_pipeline.yml",
            "vars": [
                {"name": "trigger_on_repo_change", "value": "false"}
            ]
        },
        {
            "name": "destroy_env",
            "file": "ci/pipelines/destroy_pipeline.yml"
        }
    ],

    "vars": [
        {"name": "bosh_public_key", "fortress_var": "bosh_public_key"},
        {"name": "bosh_subnet_cidr", "value": "10.91.1.0/24"},
        {"name": "cf_01_subnet_cidr", "value": "10.91.16.0/20"},
        {"name": "cf_02_subnet_cidr", "value": "10.91.32.0/20"},

        {"name": "cf_certificate_arn", "fortress_var": "cf_certificate_arn"},
        {"name": "concourse_certificate_arn", "fortress_var": "concourse_certificate_arn"},

        {"name": "dmz_01_subnet_cidr", "value": "10.91.2.0/24"},
        {"name": "dmz_02_subnet_cidr", "value": "10.91.3.0/24"},

        {"name": "jumpbox_ami", "fortress_var": "jumpbox_ami"},
        {"name": "jumpbox_public_key", "fortress_var": "jumpbox_public_key"},
        {"name": "jumpbox_git_private_key", "fortress_var": "git_private_key"},

        {"name": "network_cidr", "value": "10.91.0.0/16"},

        {"name": "ops_subnet_cidr", "value": "10.91.0.0/24"},

        {"name": "tools_01_subnet_cidr", "value": "10.91.10.0/24"},
        {"name": "tools_02_subnet_cidr", "value": "10.91.11.0/24"},

        {"name": "git_private_key", "fortress_var": "git_private_key"}
    ]
}
