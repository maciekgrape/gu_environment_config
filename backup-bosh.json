{
    "repo_url": "git@github.com:grapeup/gu_bosh_deployment.git",
    "repo_private_key_var_name": "git_private_key",
    "repo_version": "master",
    "file": "ci/pipelines/backup_pipeline.yml",

    "credhub_vars": [
        {"name": "bosh_state_s3_access_key", "path": "/fortress/aws_access_key"},
        {"name": "bosh_state_s3_secret_key", "path": "/fortress/aws_secret_key"},
        {"name": "bosh_state_s3_region", "path": "/fortress/aws_region"},

        {"name": "bosh_deployment_repo_private_key", "path": "/fortress/git_private_key"},

        {"name": "bosh_backup_bucket_name", "path": "/fortress/backups_bucket_id"},
        {"name": "bosh_backup_bucket_region", "path": "/fortress/aws_region"},
        {"name": "bosh_backup_s3_access_key", "path": "/fortress/aws_access_key"},
        {"name": "bosh_backup_s3_secret_key", "path": "/fortress/aws_secret_key"}
    ],

    "raw_vars": [
        {"name": "env_name", "value": "bosh-dev"},
        {"name": "bosh_deployment_version", "value": "master"},

        {"name": "bosh_state_bucket_name", "value": "bosh-state.bosh-dev.cflab.grapeup.com"},
        {"name": "bosh_state_filename", "value": "bosh-dev.tar"},

        {"name": "backup_interval", "value": "12h"},
        {"name": "system_domain", "value": "bosh-dev.cflab.grapeup.com"},
        {"name": "bosh_instance_ip", "value": "10.92.1.6"}
    ]
}
